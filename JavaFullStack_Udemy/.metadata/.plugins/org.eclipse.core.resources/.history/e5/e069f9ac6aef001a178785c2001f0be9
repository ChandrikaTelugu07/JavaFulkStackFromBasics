package arthematicoperators.examples;

public class Operators {
	// to perform some operations 
	
	public static void main(String[] args) {
		 int x = 10;
		 System.out.println(x);
		 
		 // +
		 int a = 10;
		 int b = 20;
		 System.out.println(a+b);
		 
		 String s1 = "Hello";
		 String s2 = "world";
		 System.out.println(s1+s2);
	}

}
