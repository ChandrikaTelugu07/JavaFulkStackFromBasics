package s4p01_operators.examples;

public class TernaryOperator {

	public static void main(String[] args) {
		boolean x ;
		boolean y;
		x = (5>4)?true: false;
		System.out.println(x);
		
		y = (5>9)?true: false;
		System.out.println(y);
		
		
		int a ;
		
		a = (10==9)? 1 : 0;
		System.out.println(a);

	}

}
