package operators.examples;

public class AssignmentOperator {

	public static void main(String[] args) {
		int x = 5;
		
		System.out.println(x+=5);// x =10
		System.out.println(x-=5);// x =5
		System.out.println(x*=5);// x =25
		System.out.println(x/=5);// x= 5
		System.out.println(x%=5);//x =0
		

	}

}
